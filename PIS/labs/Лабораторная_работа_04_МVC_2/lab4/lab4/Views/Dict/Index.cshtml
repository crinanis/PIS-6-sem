@using lab4.Models;

@{
    Layout = null;
    List<PhoneBook> book = ViewBag.PhoneBook;
}

<!DOCTYPE html>

<html>
<head>
    <title>Index</title>
</head>
<body>
    <div>
        <table>
            <thead>
                <tr>
                    <th>id</th>
                    <th>surname</th>
                    <th>phone number</th>
                </tr>
            </thead>
            @foreach (PhoneBook row in book)
             {
                <tr>
                    <td>@row.id</td>
                    <td>@row.surname</td>
                    <td>@row.phoneNumber</td>
                </tr>
             }
        </table>
    </div>
    <h5> <a href="~/Dict/Add"> Add </a></h5>
    <h5> <a href="~/Dict/Update"> Update </a></h5>
    <h5> <a href="~/Dict/Delete"> Delete </a></h5>
</body>
</html>
